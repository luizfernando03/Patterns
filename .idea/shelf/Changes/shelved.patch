+package model.Descontos;
+
+import model.Orcamento;
+
+import java.math.BigDecimal;
+
+public class DescontoQuantidadeDeIntens extends Desconto{
+    public DescontoQuantidadeDeIntens(Desconto proximo) {
+        super (proximo);
+    }
+
+    public BigDecimal calcular(Orcamento orcamento) {
+        if (orcamento.getQtdItens (new BigDecimal ("500")) > 5) {
+            return orcamento.getValorOrcamento ().multiply (new
+                    BigDecimal ("0.1"));
+
+        } else {
+            return proximo.calcular (orcamento);
+        }
+    }
+}
